{
  "name": "znck/livre",
  "type": "library",
  "description": "Everything with books.",
  "homepage": "https://github.com/znck/livre",
  "keywords": ["laravel", "books", "isbn", "library", "catalog", "classification"],
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.0",
    "illuminate/support": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "graham-campbell/testbench": "^3.1",
    "fabpot/php-cs-fixer": "^1.11",
    "phpunit/php-code-coverage": "^3.3",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Znck\\Livre\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Znck\\Tests\\Livre\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Rahul Kadyan",
      "email": "hi@znck.me"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit",
    "cs": "php-cs-fixer fix"
  }
}
